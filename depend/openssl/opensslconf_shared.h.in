#ifndef _OPENSSLCONF_SHARED_H_
#define _OPENSSLCONF_SHARED_H_

#if __APPLE__
    #include <TargetConditionals.h>
    #error "Not supported Apple platform"
#elif defined(ANDROID) || defined(__ANDROID__)
    #if defined(__arm__)
        #include "android/opensslconf-armeabi-v7a.h"
    #elif defined(__aarch64__)
        #include "android/opensslconf-arm64-v8a.h"
    #elif defined(__i386__)
        #include "android/opensslconf-x86.h"
    #elif defined(__x86_64__)
        #include "android/opensslconf-x86_64.h"
    #else
        #error "Not supported Android platform"
    #endif
#elif __linux__
    #if defined(__LP64__) && __LP64__
        #include "linux/opensslconf-x86_64.h"
    #else
        #include "linux/opensslconf-x86.h"
    #endif
#else
    #error "Not supported platform"
#endif

#endif
